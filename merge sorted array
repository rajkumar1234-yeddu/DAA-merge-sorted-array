#include<stdio.h>

int merge(int* nums1, int nums1Size, int m, int* nums2, int nums2Size, int n){

    int nums[nums1Size];
    int i=0, j=0, k=0;
    while(i<m && j<n){
        if (nums1[i]<nums2[j]){
            nums[k++]=nums1[i++];
        }
        else{
            nums[k++]=nums2[j++];
        }
    }
    for(; i<m; i++){
        nums[k++]=nums1[i];
    }
    for(; j<n; j++){
        nums[k++]=nums2[j];
    }
    for(int i=0; i<nums1Size; i++){
        nums1[i]=nums[i];
    }
}

int main(){
    int n1,m1,n,m,a,b;
    printf("Enter size of array 1 : ");
    scanf("%d",&n1);
    int nums1[n1];
    printf("Enter array 1 elements :\n");
    for(int i=0;i<n1;i++){
        scanf("%d",&a);
        nums1[i]=a;
    }
    printf("Enter size of array 2 : ");
    scanf("%d",&m1);
    int nums2[m1];
    printf("Enter array 2 elements :\n");
    for(int i=0;i<m1;i++){
        scanf("%d",&a);
        nums2[i]=a;
    }
    printf("Enter n, m values :");
    scanf("%d %d",&n,&m);
    merge(nums1,n1,n,nums2,m1,m);
    
    for(int i=0; i<n1; i++){
        printf("%d ",nums1[i]);
    }
}
